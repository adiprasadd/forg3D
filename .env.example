# RPC Provider URL for Story Protocol
# Default: https://aeneid.storyrpc.io
NEXT_PUBLIC_RPC_PROVIDER_URL=

# Story Protocol Chain ID
# Default: aeneid
NEXT_PUBLIC_STORY_CHAIN_ID=

# Contract Addresses
# Default NFT Contract: 0x937bef10ba6fb941ed84b8d249abc76031429a9a
NEXT_PUBLIC_NFT_CONTRACT_ADDRESS=
NEXT_PUBLIC_SPG_NFT_CONTRACT_ADDRESS=

# Wallet Configuration
# IMPORTANT: Never commit your actual private key!
# Format: Your wallet private key without the '0x' prefix
WALLET_PRIVATE_KEY=

# Story Protocol Configuration
STORY_API_KEY=your_api_key
STORY_RPC_URL=your_rpc_url_here

# Deployment
PRIVATE_KEY=your_private_key_here
ETHERSCAN_API_KEY=your_etherscan_api_key

# Network Configuration
NETWORK=testnet  # or mainnet

# Cloudinary Configuration
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret
CLOUDINARY_URL=cloudinary://<api_key>:<api_secret>@<cloud_name>

# Example values:
# NEXT_PUBLIC_RPC_PROVIDER_URL=https://aeneid.storyrpc.io
# NEXT_PUBLIC_STORY_CHAIN_ID=aeneid
# NEXT_PUBLIC_NFT_CONTRACT_ADDRESS=0x937bef10ba6fb941ed84b8d249abc76031429a9a
# NEXT_PUBLIC_SPG_NFT_CONTRACT_ADDRESS=0x...
# WALLET_PRIVATE_KEY=abcdef1234... 
# NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
# NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1...

# Supabase Storage Setup:
# 1. Create a bucket named '3d_files' in Supabase Storage
# 2. Configure CORS for the bucket to allow uploads from your domain
# 3. Add the following storage policy to allow public access:
#    - Policy name: "Allow public access to 3d_files"
#    - Operations: SELECT, INSERT, UPDATE, DELETE
#    - Target roles: public
#    - Policy definition: bucket_id = '3d_files'