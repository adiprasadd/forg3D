# RPC Provider URL for Story Protocol
# Default: https://aeneid.storyrpc.io
NEXT_PUBLIC_RPC_PROVIDER_URL=

# Story Protocol Chain ID
# Default: aeneid
NEXT_PUBLIC_STORY_CHAIN_ID=

# Contract Addresses
# Default NFT Contract: 0x937bef10ba6fb941ed84b8d249abc76031429a9a
NEXT_PUBLIC_NFT_CONTRACT_ADDRESS=
NEXT_PUBLIC_SPG_NFT_CONTRACT_ADDRESS=

# Wallet Configuration
# IMPORTANT: Never commit your actual private key!
# Format: Your wallet private key without the '0x' prefix
WALLET_PRIVATE_KEY=

# Story Protocol Configuration
STORY_API_KEY=your_api_key_here
STORY_RPC_URL=your_rpc_url_here

# Deployment
PRIVATE_KEY=your_private_key_here
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Network Configuration
NETWORK=testnet  # or mainnet

# Example values:
# NEXT_PUBLIC_RPC_PROVIDER_URL=https://aeneid.storyrpc.io
# NEXT_PUBLIC_STORY_CHAIN_ID=aeneid
# NEXT_PUBLIC_NFT_CONTRACT_ADDRESS=0x937bef10ba6fb941ed84b8d249abc76031429a9a
# NEXT_PUBLIC_SPG_NFT_CONTRACT_ADDRESS=0x...
# WALLET_PRIVATE_KEY=abcdef1234... 